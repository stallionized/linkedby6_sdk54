{
  "expo": {
    "name": "Linked By Six",
    "slug": "linked-by-six",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "linkedbysix",
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linkedbysix.app",
      "newArchEnabled": false,
      "icon": {
        "light": "./assets/icons/ios-light.png",
        "dark": "./assets/icons/ios-dark.png",
        "tinted": "./assets/icons/ios-tinted.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.linkedbysix.app",
      "newArchEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "windowSoftInputMode": "adjustResize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#2196F3"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.linkedbysix.app",
          "enableGooglePay": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "stripeServerUrl": "https://stripeserver-2w6d.onrender.com",
      "eas": {
        "projectId": "dd7711ca-c7b6-4066-bd17-27f06af90a39"
      }
    }
  }
}
